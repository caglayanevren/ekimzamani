<!DOCTYPE html>
<html lang="{{#eq language 'tr'}}tr{{else}}en{{/eq}}">
{{> head}}

<body {{#if bodyClass}}class="{{ bodyClass }}" {{/if}}>
    {{#eq language 'tr'}}
    <a href="#main-content" class="skip-link">Ana içeriğe atla</a>
    {{> browser-upgrade-tr}}
    <header>
        {{> sabanci-vakfi-header}}
        {{> navigasyon}}
    </header>
    {{else}}
    <a href="#main-content" class="skip-link">Skip to main content</a>
    {{> browser-upgrade-en}}
    <header>
        {{> sabanci-vakfi-header}}
        {{> navigation}}
    </header>
    {{/eq}}
    <!-- /main-container -->
    {{#block "main"}}
    {{/block}}
    <!-- /main-container -->
    {{> sabanci-vakfi-footer}}
    <script src="{{ script }}"></script>
</body>

</html>